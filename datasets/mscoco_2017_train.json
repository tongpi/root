{
  "name": "MSCOCO_2017_Train",
  "description": "2017 MSCOCO training set",
  "process_type": "V",
  "create": [
    {
      "MODEL": "Video",
      "spec": {
        "name": "2017 MSCOCO training set",
        "dataset": true
      },
      "tasks": [
        {
          "operation": "perform_import",
          "video_id": "__pk__",
          "arguments": {
            "path": "http://192.168.200.106/download/train2017.zip",
            "map": [
              {
                "operation": "perform_dataset_extraction",
                "arguments": {
                "map": [
                    {
                      "operation": "perform_detection",
                      "arguments": {
                        "frames_batch_size": 500,
                        "map": [
                          {
                            "operation": "perform_indexing",
                            "arguments": {
                              "index": "inception",
                              "target": "regions",
                              "filters": {
                                "event_id": "__parent_event__",
                                "w__gte": 50,
                                "h__gte": 50
                              },
                              "map": [
                                {
                                  "operation": "perform_index_approximation",
                                  "arguments": {
                                    "target": "index_entries",
                                    "approximator_shasum": "86689432ac09d86dd95d7d3ed57227d27eb09c81",
                                    "filters": {
                                      "event_id": "__parent_event__"
                                    }
                                  }
                                }
                              ]
                            }
                          }
                        ],
                        "detector": "coco"
                      }
                    },
                    {
                      "operation": "perform_detection",
                      "arguments": {
                        "frames_batch_size": 500,
                        "map": [
                          {
                            "operation": "perform_indexing",
                            "arguments": {
                              "index": "facenet",
                              "target": "regions",
                              "filters": {
                                "event_id": "__parent_event__"
                              },
                              "map": [
                                {
                                  "operation": "perform_index_approximation",
                                  "arguments": {
                                    "target": "index_entries",
                                    "approximator_shasum": "93be2f99d432a7ce2d1626107659510755ea3815",
                                    "filters": {
                                      "event_id": "__parent_event__"
                                    }
                                  }
                                }
                              ]
                            }
                          }
                        ],
                        "detector": "face"
                      }
                    },
                    {
                      "operation": "perform_indexing",
                      "arguments": {
                        "frames_batch_size": 500,
                        "index": "inception",
                        "target": "frames",
                        "map": [
                          {
                            "operation": "perform_index_approximation",
                            "arguments": {
                              "target": "index_entries",
                              "approximator_shasum": "86689432ac09d86dd95d7d3ed57227d27eb09c81",
                              "filters": {
                                "event_id": "__parent_event__"
                              }
                            }
                          }
                        ]
                      }
                    },
                    {
                      "operation": "perform_analysis",
                      "arguments": {
                        "frames_batch_size": 500,
                        "target": "frames",
                        "analyzer": "tagger"
                      }
                    },
                    {
                      "operation": "perform_region_import",
                      "arguments": {
                        "path": "http://192.168.200.106/download/coco_train2017_keypoints_regions.gz"
                      }
                    },
                    {
                      "operation": "perform_region_import",
                      "arguments": {
                        "path": "http://192.168.200.106/download/coco_train2017_captions_regions.gz"
                      }
                    },
                    {
                      "operation": "perform_region_import",
                      "arguments": {
                        "path": "http://192.168.200.106/download/coco_train2017_instances_regions.gz"
                      }
                    }                    
                  ]
                }
              }
            ]
          }
        }
      ]
    }
  ]
}
